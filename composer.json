{
    "name": "jelmerd/file-explorer",
    "description": "FileExplorer plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "JelmerD\\FileExplorer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JelmerD\\FileExplorer\\Test\\": "tests/"
        }
    }
}
